<?xml version="1.0" encoding="UTF-8" ?>  
<!DOCTYPE mapper 
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="demo">

	<resultMap type="com.tx.webdemo.demo.model.Demo" id="demoResultMap">
		<result property="name" column="name"/>
		<result property="" column=""/>
		<result property="" column=""/>
		<result property="" column=""/>
	</resultMap>

	<!-- 查询学生，根据id -->
	<select id="findDemo" parameterType="demo" resultType="demo">  
        <![CDATA[ 
            SELECT name,password,email,createDate 
              FROM t_test_demo 
             WHERE name = #{name}  
        ]]>
        <trim >
        
        </trim>
	</select>

	<insert id="insertDemo" 
		parameterType="demo">
		<selectKey keyProperty="id" resultType="java.util.String">
			select
			${@java.util.UUID@randomUUID} from dual
		</selectKey>
		insert into t_test_demo
		(
			name,
			password,
			<if test="${subDemo}">
				<if test="@org.apache.commons.lang.StringUtils@isNotEmpty(subDemo.id)">
					demoId
				</if>
			</if>
			email,
			createDate
		)
		values
		(	
			#{name},
			#{password},
			#{email},
			#{createDate}
		)
	</insert>
	
	<delete id="" parameterType="demo">
		delete from wd_demo d
		where 
		<trim prefixOverrides="and | or">
			<if test="">
				
			</if>
		</trim>
	</delete>
	
	<update id="">
		
	</update>

</mapper>  